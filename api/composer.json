{
  "name": "assembly/project",
  "type": "project",
  "license": "BSD-3-Clause",
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "require": {
    "php": "^8.0",
    "doctrine/migrations": "^3.5",
    "doctrine/orm": "^2.14",
    "laminas/laminas-config-aggregator": "^1.13",
    "monolog/monolog": "^2.9",
    "php-di/php-di": "^7.0",
    "slim/psr7": "^1.6",
    "slim/slim": "4.*",
    "symfony/cache": "^6.0",
    "symfony/console": "^6.0"
  },
  "require-dev": {
    "doctrine/data-fixtures": "^1.6",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "scripts": {
    "app": "php bin/app.php --ansi"
  }
}
